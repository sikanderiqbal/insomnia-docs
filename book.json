{
  "gitbook": "3.x.x",
  "title": "Insomnia HTTP Client",
  "root": "docs",
  "structure": {
    "summary": "SUMMARY.md",
    "readme": "README.md"
  },
  "plugins": [
    "-highlight",
    "-fontsettings",
    "prism",
    "github",
    "anker-enable",
    "edit-link",
    "ga"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/gschier/insomnia-docs/tree/master",
      "label": "Edit This Page"
    },
    "github": {
      "url": "https://github.com/gschier/insomnia-docs"
    },
    "ga": {
      "token": "UA-9837747-12",
      "configuration": {
        "allowLinker": true
      }
    }
  }
}